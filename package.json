{
  "name": "react-lru",
  "version": "0.1.2",
  "description": "Least recently used (LRU) cache algorithm rendering of React roots",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha test/dom-mock.js test/**/*-test.js --compilers js:babel-core/register",
    "build": "babel src --out-dir lib",
    "bundle": "NODE_ENV=production browserify src/index.js -o lib/bundle.js -t [ babelify --presets [ latest react stage-0 ] ] | uglifyjs lib/bundle.js > lib/bundle.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zekedroid/react-lru.git"
  },
  "keywords": [
    "lru",
    "react",
    "react-dom",
    "infinite",
    "scroll",
    "table",
    "handsontable"
  ],
  "author": {
    "name": "Zeke Schmois",
    "email": "zekedroid@gmail.com"
  },
  "contributors": [
    {
      "name": "Jackson Westeen"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zekedroid/react-lru/issues"
  },
  "homepage": "https://github.com/zekedroid/react-lru#readme",
  "dependencies": {
    "js-lru": "^1.0.0",
    "lodash": "^4.17.4",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "chai": "^4.1.2",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "jsdom": "9.4.2",
    "mocha": "^4.0.1",
    "react": "15.6.2",
    "react-dom": "15.6.2",
    "sinon": "^4.0.1",
    "uglify-js": "^3.1.3"
  },
  "peerDependencies": {
    "react": "^0.14 || ^15 || ^16",
    "react-dom": "^0.14 || ^15 || ^16"
  }
}
